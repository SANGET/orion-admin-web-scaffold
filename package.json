    "mousetrap": "^1.6.1",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.0",
    "uke-request": "^0.12.x",
    "ukelli-ui": "^2.7.x"
  },
