{
  "name": "orion-admin-web-template",
  "version": "0.3.3",
  "main": "./index.js",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm run build; npm run publish",
    "init-workspace": "npm run init-manager-template; npm run init-fe-deploy",
    "init-manager-template": "cd ./packages/orion-manager-web; git clone https://github.9th.go/frontend-orion/orion-admin-web-template.git",
    "init-fe-deploy": "cd ./packages/fe-deploy; git clone https://github.9th.go/matrix-front-end/fe-deploy.git",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files; npm run copy-style",
    "copy-style": "cp -R ./style ./build/",
    "build:babel": "NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "NODE_ENV=production babel-node ./scripts/copy-files.js",
    "push": "npm publish; git add -A; git commit -m 'update'; git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "http://github.9th.go/frontend-orion/orion-admin-web-template.git"
  },
  "description": "",
  "peerDependencies": {
    "ukelli-ui": "^2.4.x",
    "cross-env": "^5.1.6",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.0"
  },
  "dependencies": {
    "mousetrap": "^1.6.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.0.2"
  },
  "author": "alex",
  "license": "ISC"
}
