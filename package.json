    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "basic-helper": "^1.4.4",
    "docz": "^0.12.9",
    "docz-plugin-netlify": "^1.1.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "fs-extra": "^7.0.0",
    "jest": "^23.6.0",
