{
  "name": "orion-admin-web-template",
  "version": "0.1.1",
  "main": "./index.js",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm run build; npm run publish",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files; npm run copy-style",
    "copy-style": "cp -R ./style ./build/",
    "build:babel": "cross-env NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "cross-env NODE_ENV=production babel-node ./scripts/copy-files.js",
    "push": "npm publish; git add -A; git commit -m 'update'; git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SANGET/orion-admin-web-scaffold.git"
  },
  "description": "",
  "peerDependencies": {
    "ukelli-ui": "^2.2.0",
    "cross-env": "^5.1.6"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "mousetrap": "^1.6.1",
    "react-transition-group": "^2.2.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.0.2"
  },
  "author": "alex",
  "license": "ISC"
}
