{
  "name": "uke-admin-web-scaffold",
  "version": "0.11.1",
  "main": "./index.js",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm run build; npm run publish",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files; npm run copy-style",
    "copy-style": "cp -R ./style ./build/",
    "build:babel": "NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "NODE_ENV=production babel-node ./scripts/copy-files.js",
    "push": "npm publish; git add -A; git commit -m 'update'; git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "http://github.9th.go/frontend-uke/uke-admin-web-scaffold.git"
  },
  "description": "",
  "peerDependencies": {
    "cross-env": "^5.1.6",
    "mousetrap": "^1.6.1",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-transition-group": "^2.2.0",
    "ukelli-ui": "^2.5.x"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "fs-extra": "^7.0.0"
  },
  "author": "alex",
  "license": "ISC",
  "dependencies": {
    "history": "^4.7.2"
  }
}
